{
  "name": "vteemo-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "echo \"(add linter later)\"",
    "migrate:latest": "knex --knexfile knexfile.cjs migrate:latest",
    "migrate:rollback": "knex --knexfile knexfile.cjs migrate:rollback",
    "migrate:make": "knex --knexfile knexfile.cjs migrate:make",
    "migrate": "npm run migrate:latest",
    "media:migrate": "npm run migrate:latest",
    "worker:video": "node dist/workers/videoProcessor.js",
    "gen:secrets": "ts-node-dev --transpile-only scripts/generate-secrets.ts",
    "start:auto": "node dist/autoStart.js"
  },
  "dependencies": {
    "bullmq": "^5.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "mysql2": "^3.9.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "zod": "^3.23.8",
    "@aws-sdk/client-s3": "^3.645.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "mime-types": "^2.1.35",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "swagger-ui-express": "^5.0.1",
    "@ffprobe-installer/ffprobe": "^1.4.5",
    "prom-client": "^15.1.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "ua-parser-js": "^1.0.37",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/swagger-ui-express": "^4.1.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "@types/multer": "^1.4.11",
    "@types/mime-types": "^2.1.4",
    "@types/nodemailer": "^6.4.13",
    "@types/bcrypt": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
